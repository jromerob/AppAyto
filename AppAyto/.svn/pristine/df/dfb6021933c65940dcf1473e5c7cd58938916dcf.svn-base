<ion-view view-title="Info ">
    <ion-content>


        <div class="list">

            <a class="item item-thumbnail-left" href="#">
                <img src="cover.jpg">
            </a>




            <div class="list card">

                <a href="#" class="item item-icon-left">
                    <i class="icon ion-information-circled"></i>
                    {{remoteConfig.versionName}}
                </a>

                <a href="#" class="item item-icon-left">
                    <i class="icon ion-email"></i>
                    {{remoteConfig.email.general}}
                </a>

                <a href="#" class="item item-icon-left">
                    <i class="icon ion-email-unread"></i>
                    {{remoteConfig.email.police}}
                </a>

                <a href="#" class="item item-icon-left">
                    <i class="icon ion-ios-telephone"></i>
                    {{remoteConfig.telephone.centralita}}
                </a>

            </div>


        </div>
    </ion-content>
</ion-view>
